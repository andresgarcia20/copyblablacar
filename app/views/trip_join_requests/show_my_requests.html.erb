<h1>My trip join requests</h1>

<div id="trip_join_requests">
  <% @trip_join_requests.each do |trip_join_request| %>
    <%= render trip_join_request %>
    <p>
      <%= link_to "Show this trip join request", trip_trip_join_request_path(trip_join_request.trip, trip_join_request) %> |
      <%= link_to "Show this trip", trip_path(trip_join_request.trip) %>
    </p>
  <% end %>
</div>