<p style="color: green"><%= notice %></p>

<h1>Vehicles</h1>

<div id="vehicles">
  <% @vehicles.each do |vehicle| %>
    <%= render vehicle %>
    <p>
      <%= link_to "Show this vehicle", user_vehicle_path(vehicle.user, vehicle) %>
    </p>
  <% end %>
</div>

<%= link_to "New vehicle", new_user_vehicle_path %>
